generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       Int     @id @default(autoincrement())
  username String  @unique
  email    String  @unique
  password String
}

model Producto {
  id          Int    @id @default(autoincrement())
  titulo      String
  img         String
}
